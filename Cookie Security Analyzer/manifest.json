{
    "name": "Cookie Security Analyzer",
    "description": "Analyzes website's cookies and gives its security level with the recommendations.",
    "version": "1.1",
    "manifest_version": 3,
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "cookies", 
        "webNavigation",
        "storage"
    ],
    "background":{
        "service_worker": "background.js"
    },
    "host_permissions": [
        "<all_urls>"
    ]
}
